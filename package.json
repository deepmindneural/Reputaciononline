{
  "name": "reputacion-online",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS='--max_old_space_size=4096 --openssl-legacy-provider' next build",
    "static": "next build && next export -o dist",
    "start": "next start",
    "lint": "next lint",
    "prisma:generate": "prisma generate --schema=./backend/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./backend/prisma/schema.prisma",
    "prisma:deploy": "prisma migrate deploy --schema=./backend/prisma/schema.prisma",
    "db:setup": "npm run prisma:generate && npm run prisma:migrate",
    "db:studio": "prisma studio --schema=./backend/prisma/schema.prisma"
  },
